<view class='btn_content'>
    <button size='mini' catchtap=''>普通标点</button>
    <button size='mini' catchtap=''>展示</button>
    <button size='mini' catchtap=''>导航开始</button>
    <button size='mini' catchtap=''>导航结束</button>
</view>
<stage
    list="{{pointList}}"
    dir="{{direction}}"
    z="{{accZ}}"
    mode="{{mode}}"
    focus="{{focusList}}"
>
</stage>


